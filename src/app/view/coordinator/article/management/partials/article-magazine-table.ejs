<div class="rounded h-100 p-4">
    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
        <% magazine_list.map((magazine, index) => { %>
            <li class="nav-item" role="presentation">
                <button class="nav-link <%= index === 0 ? 'active' : '' %>"
                        data-bs-toggle="pill" data-bs-target="#magazine-<%= index %>"
                        type="button" role="tab" aria-controls="pills-home" aria-selected="true">
                    <%
                        let icon = 'fa-door-open'
                        if (magazine.is_final_closure) {
                            icon = 'fa-circle-xmark'
                        } else if (magazine.is_closure) {
                            icon = 'fa-door-closed'
                        }
                    %>
                    <%= magazine.start_academic_year + '-' + magazine.end_academic_year %>
                    <i class="fa-solid <%= icon %> ms-2"></i>
                </button>
            </li>
        <% }) %>
    </ul>
    <div class="tab-content" id="pills-tabContent">
        <% magazine_list.map((magazine, index) => { %>
            <div class="tab-pane fade <%= index === 0 ? 'active show' : '' %>" id="magazine-<%= index %>"
                 role="tabpanel">
                <% if(magazine.articles) { %>
                    <%- include('./contribute-table', {contribute_list: magazine.articles}) %>
                <% } %>
            </div>
        <% }) %>
    </div>
</div>
